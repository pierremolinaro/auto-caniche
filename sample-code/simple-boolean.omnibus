
enum $bool {no, yes}

#--------------------------------------------------------*

machine @boolean
  var b $bool = .no

  event set (?inValue $bool) {
    b = inValue
  }
}

#--------------------------------------------------------*

scenario @boolean "seq" {
  set(!yes)
  set(!yes)
  set(!yes)
  set(!no)
  set(!no)
  set(!no)
}

#--------------------------------------------------------*
